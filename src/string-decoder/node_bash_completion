_node_complete() {
  local cur_word options
  cur_word="${COMP_WORDS[COMP_CWORD]}"
  if [[ "${cur_word}" == -* ]] ; then
    COMPREPLY=( $(compgen -W '--track-heap-objects --harmony-top-level-await --no-node-snapshot --interpreted-frames-native-stack --inspect-brk --max-old-space-size --trace-exit --disallow-code-generation-from-strings --experimental-json-modules --print --check --trace-tls --stack-trace-limit --debug-brk --experimental-repl-await --inspect-brk-node --report-uncaught-exception --no-force-async-hooks-checks --trace-sync-io --test-udp-no-try-send --experimental-modules --report-signal --jitless --inspect-port --prof --heapsnapshot-near-heap-limit --tls-keylog --v8-options --force-context-aware --napi-modules --abort-on-uncaught-exception --require --debug --eval --diagnostic-dir --verify-base-objects --unhandled-rejections --heap-prof-interval --perf-basic-prof --heap-prof --no-warnings --trace-atomics-wait --cpu-prof-interval --cpu-prof --perf-basic-prof-only-functions --perf-prof --max-http-header-size --prof-process --report-on-signal --throw-deprecation --cpu-prof-dir --heap-prof-name --trace-event-categories --experimental-worker --heap-prof-dir --no-deprecation --force-fips --pending-deprecation --input-type --tls-max-v1.3 --tls-min-v1.2 --inspect --heapsnapshot-signal --trace-warnings --expose-internals --tls-max-v1.2 --interactive --perf-prof-unwinding-info --preserve-symlinks-main --trace-event-file-pattern --trace-uncaught --http-parser --experimental-loader --experimental-wasm-modules --node-memory-debug --inspect-publish-uid --tls-min-v1.3 --experimental-specifier-resolution --secure-heap --tls-min-v1.0 --redirect-warnings --experimental-report --trace-sigint --secure-heap-min --preserve-symlinks --disable-proto --huge-max-old-generation-size --enable-fips --enable-source-maps --use-openssl-ca --insecure-http-parser --tls-cipher-list --experimental-top-level-await --openssl-config --icu-data-dir --v8-pool-size --report-on-fatalerror --title --tls-min-v1.1 --report-filename --policy-integrity --trace-deprecation --report-compact --experimental-policy --experimental-import-meta-resolve --zero-fill-buffers --report-dir --use-bundled-ca --experimental-vm-modules --version --frozen-intrinsics --help --completion-bash --cpu-prof-name --security-revert --debug-arraybuffer-allocations --conditions --experimental-wasi-unstable-preview1 --use-largepages --experimental-abortcontroller --debug-port -p -pe --debug-brk= --inspect-brk-node= -c -e --es-module-specifier-resolution --prof-process --loader --debug= --report-directory --inspect-brk= -v -h -i --security-reverts --inspect= --print <arg> -r --trace-events-enabled' -- "${cur_word}") )
    return 0
  else
    COMPREPLY=( $(compgen -f "${cur_word}") )
    return 0
  fi
}
complete -o filenames -o nospace -o bashdefault -F _node_complete node node_g
